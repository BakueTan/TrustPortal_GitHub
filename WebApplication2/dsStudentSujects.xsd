                         Subjects ON studentsubjects.SubjectID = Subjects.SubjectID INNER JOIN
                         forms ON studentsubjects.Program = forms.Forms INNER JOIN
                         enrollment ON studentsubjects.StudentID = enrollment.StudentID AND studentsubjects.Program = enrollment.Program AND studentsubjects.Year = enrollment.Year AND studentsubjects.Semester = enrollment.Semester
WHERE        (studentsubjects.Year = @level) AND (studentsubjects.Semester = @sem) AND (studentsubjects.Program = @prog) AND (studentsubjects.StudentID = @stud) AND (studentsubjects.RegStatus = 'Registered')</CommandText>
